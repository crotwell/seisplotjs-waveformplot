{
  "name": "seisplotjs-waveformplot",
  "version": "1.2.2-alpha.2",
  "description": "Plotting seismic data in miniseed format using d3.",
  "main": "./lib/waveformplot.js",
  "dependencies": {
    "d3": "^4.10.0",
    "seisplotjs-fdsndataselect": ">= 1.0.0",
    "seisplotjs-miniseed": ">= 1.2.0-alpha.2"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/*.js --standalone seisplotjs_waveformplot -o seisplotjs_waveformplot_${npm_package_version}_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-waveformplot.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
    "d3",
    "seismogram"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-waveformplot/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-waveformplot",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
