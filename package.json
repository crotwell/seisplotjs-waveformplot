{
  "name": "seisplotjs-waveformplot",
  "version": "1.2.2-alpha.3",
  "description": "Plotting seismic data in miniseed format using d3.",
  "main": "./lib/waveformplot.js",
  "dependencies": {
    "d3": "^4.12.2",
    "seisplotjs-fdsndataselect": "^1.1.2-alpha.1",
    "seisplotjs-miniseed": "^1.2.0"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel  -d lib/ src/",
    "standalone": "browserify src/index.js --standalone seisplotjs_waveformplot -o seisplotjs_waveformplot_${npm_package_version}_standalone.js -t [ babelify --presets [ env ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-waveformplot.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
    "d3",
    "seismogram"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-waveformplot/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-waveformplot",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
